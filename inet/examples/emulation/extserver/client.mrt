# filename: client.mrt
# author: Thomas Reschka
# routing table for client of tcp example "extserver"

ifconfig:

# interface 0 to router
name: ppp0
    inet_addr: 172.0.0.1
    Mask: 255.255.255.0
    MTU: 1500
    POINTTOPOINT MULTICAST

ifconfigend.


route:

#Destination     Gateway          Genmask          Flags  Metric  Iface
default:         172.0.0.100      0.0.0.0          G      0       ppp0

routeend.
